{
  "schemaVersion": 1,
  "id": "qu-enchantments",
  "version": "${version}",

  "name": "Qu Enchantments",
  "description": "Adds some of my custom enchantments into the game.",
  "authors": [
    "Qu (Design and programming)",
    "E_leven (Texture artist)"
  ],
  "contact": {
    "homepage": "https://github.com/Quplet/Qu-Enchantments",
    "sources": "https://github.com/Quplet/Qu-Enchantments",
    "issues": "https://github.com/Quplet/Qu-Enchantments/issues"
  },

  "license": "Apache-2.0",
  "icon": "assets/qu-enchantments/icon.png",

  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "qu.quEnchantments.QuEnchantments"
    ],
    "main": [
      "qu.quEnchantments.QuEnchantments"
    ],
    "mm:early_risers": [
      "qu.quEnchantments.asm.EarlyRiser"
    ],
    "client": [
      "qu.quEnchantments.QuEnchantments"
    ],
    "modmenu": [
      "qu.quEnchantments.util.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "qu-enchantments.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.17",
    "fabric": ">=0.83.0+1.20",
    "minecraft": ">=1.20-rc.1",
    "java": ">=17",
    "cloth-config": ">=11.0.98"
  },
  "suggests": {
    "another-mod": "*"
  },
  "accessWidener" : "qu-enchantments.accesswidener"
}
